<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/mainview"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@color/white"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/layout5"
        android:layout_width="match_parent"
        android:layout_height="@dimen/home2_top_layout_height"
        android:background="#e6e6e6"
        android:orientation="vertical" >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <View
                android:id="@+id/view1"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/home2_view_height"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:background="#d6d6d6" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentBottom="true"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/home2_margin_right"
                android:gravity="center" >

                <TextView
                    android:id="@+id/textView1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/home2_text_margin_left"
                    android:layout_marginRight="@dimen/home_text_margin_right"
                    android:text="FILTER"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:textColor="@color/gray_color_text"
                    android:textSize="@dimen/home_text_size"
                    android:textStyle="bold" />

                <ImageView
                    android:id="@+id/setting"
                    android:layout_width="@dimen/home2_icon_widht"
                    android:layout_height="@dimen/home2_icon_height"
                    android:src="@drawable/ico_filter" />
            </LinearLayout>
        </RelativeLayout>
    </LinearLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/layout5"
        android:orientation="vertical" >

        <RelativeLayout
            android:id="@+id/viewpager"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_above="@+id/kk" >

            <ImageView
                android:id="@+id/userProfileImage"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_above="@+id/layout4"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:layout_marginTop="15dp"
                android:scaleType="fitXY"
                android:src="@drawable/com_facebook_profile_picture_blank_square" />

            <LinearLayout
                android:id="@+id/layout4"
                android:layout_width="match_parent"
                android:layout_height="@dimen/home2_layout_height"
                android:layout_alignParentBottom="true"
                android:layout_alignParentLeft="true"
                android:gravity="center|left"
                android:orientation="vertical"
                android:paddingLeft="20dp"
                android:paddingRight="20dp" >

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:id="@+id/name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentTop="true"
                        android:text="Santosh | 90"
                        android:textColor="@color/black"
                        android:textSize="@dimen/home2_text1_size" />

                    <ImageView
                        android:id="@+id/imageOnOff"
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:layout_toLeftOf="@+id/textOnOff"
                        android:src="@drawable/online" />

                    <TextView
                        android:id="@+id/textOnOff"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:text="ONLINE"
                        android:textColor="@color/home2_3_text_color"
                        android:textSize="15sp" />
                </RelativeLayout>

                <TextView
                    android:id="@+id/address"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:text="Address here, Newyork"
                    android:textColor="@color/home2_3_text_color"
                    android:textSize="@dimen/home2_text2_size" />

                <TextView
                    android:id="@+id/commonFriends"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="2dp"
                    android:text="Friends in common"
                    android:textColor="@color/home2_3_text_color"
                    android:textSize="@dimen/home2_text3_size" />
            </LinearLayout>
        </RelativeLayout>

        <LinearLayout
            android:id="@+id/kk"
            android:layout_width="match_parent"
            android:layout_height="@dimen/home2_layout1_height"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:orientation="vertical" >

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/home2_layout1_height"
                android:background="#AF272B" >

                <ImageView
                    android:id="@+id/cancel"
                    android:layout_width="@dimen/home2_icon1_widht"
                    android:layout_height="@dimen/home2_icon1_height"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/home2_margin_Left"
                    android:src="@drawable/btn_cross" />

                <ImageView
                    android:id="@+id/add"
                    android:layout_width="@dimen/home2_icon1_widht"
                    android:layout_height="@dimen/home2_icon1_height"
                    android:layout_centerInParent="true"
                    android:src="@drawable/btn_plus" />

                <ImageView
                    android:id="@+id/done"
                    android:layout_width="@dimen/home2_icon1_widht"
                    android:layout_height="@dimen/home2_icon1_height"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/home2_margin_Right"
                    android:src="@drawable/btn_tick" />
            </RelativeLayout>
        </LinearLayout>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/congratulationLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="#cc000000" >

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView11"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="@dimen/home2_margin_top"
            android:text="You have a match."
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#ffffff"
            android:textSize="@dimen/home2_text4_size" />

        <LinearLayout
            android:id="@+id/linearLayout1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/textView2"
            android:layout_marginTop="@dimen/home2_margin11_top"
            android:gravity="center" >

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/home2_icon_margin" >

                <ImageView
                    android:id="@+id/userImageViewcongratulation"
                    android:layout_width="@dimen/home2_icon2_widht"
                    android:layout_height="@dimen/home2_icon2_height"
                    android:scaleType="fitXY"
                    android:src="@drawable/com_facebook_profile_picture_blank_portrait" />

                <ImageView
                    android:id="@+id/userImageViewcongratulationStamp"
                    android:layout_width="@dimen/home2_icon2_widht"
                    android:layout_height="@dimen/home2_icon2_height"
                    android:scaleType="fitXY"
                    android:src="@drawable/stamp"
                    android:visibility="gone" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >

                <ImageView
                    android:id="@+id/dudeImageView"
                    android:layout_width="@dimen/home2_icon2_widht"
                    android:layout_height="@dimen/home2_icon2_height"
                    android:scaleType="fitXY"
                    android:src="@drawable/com_facebook_profile_picture_blank_portrait" />

                <ImageView
                    android:id="@+id/dudeImageViewStamp"
                    android:layout_width="@dimen/home2_icon2_widht"
                    android:layout_height="@dimen/home2_icon2_height"
                    android:scaleType="fitXY"
                    android:src="@drawable/stamp"
                    android:visibility="gone" />
            </RelativeLayout>
        </LinearLayout>

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="@dimen/home2_margin111_top"
            android:text="Congratulations!"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="#ffffff"
            android:textSize="@dimen/home2_text5_size" />

        <Button
            android:id="@+id/sendmsgbtn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/linearLayout1"
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="@dimen/home2_margin1111_left"
            android:layout_marginRight="@dimen/home2_margin1111_right"
            android:layout_marginTop="@dimen/home2_margin1111_top"
            android:background="@drawable/buttonred"
            android:text="SEND HIM A MESSAGE"
            android:textColor="#ffffff" />

        <Button
            android:id="@+id/keepSearchingButton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/sendmsgbtn"
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="@dimen/home2_margin2222_left"
            android:layout_marginRight="@dimen/home2_margin2222_right"
            android:layout_marginTop="@dimen/home2_margin2222_top"
            android:background="@drawable/buttonblack"
            android:text="KEEP SEARCHING"
            android:textColor="#ffffff" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/swipeLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/home2_height"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="180dp"
            android:gravity="center"
            android:orientation="horizontal" >

            <ImageView
                android:id="@+id/cancelSwipe"
                android:layout_width="@dimen/home2_icon1_60"
                android:layout_height="@dimen/home2_icon1_60"
                android:src="@drawable/swipe_cross" />

            <ImageView
                android:id="@+id/imageView1"
                android:layout_width="100dp"
                android:layout_height="60dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:src="@drawable/arrow_w" />

            <ImageView
                android:id="@+id/doneSwipe"
                android:layout_width="@dimen/home2_icon1_60"
                android:layout_height="@dimen/home2_icon1_60"
                android:src="@drawable/swipe_right" />
        </LinearLayout>
    </RelativeLayout>

</RelativeLayout>